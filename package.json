{
  "name": "monzo-budgeter",
  "version": "0.0.1",
  "description": "Budget helper application for Monzo",
  "main": "src/server.js",
  "scripts": {
    "migratedb": "node_modules/.bin/sequelize db:migrate",
    "seeddb": "node_modules/.bin/sequelize db:seed:all",
    "initdb": "npm run -s migratedb && npm run -s seeddb",
    "run": "node src/server.js",
    "start": "npm run -s initdb & npm run -s run",
    "start-dev": "node -r dotenv/config src/server.js"
  },
  "author": "Mike McFarland",
  "license": "ISC",
  "dependencies": {
    "accounting": "^0.4.1",
    "bluebird": "^3.4.7",
    "body-parser": "^1.16.0",
    "cookie-parse": "^0.4.0",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "helmet": "^3.4.0",
    "json2csv": "^3.7.3",
    "jsonwebtoken": "^7.2.1",
    "memory-cache": "^0.1.6",
    "moment": "^2.17.1",
    "morgan": "^1.8.0",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "promise": "^7.1.1",
    "pug": "^2.0.0-beta11",
    "pug-bootstrap": "0.0.11",
    "request": "^2.79.0",
    "request-promise": "^4.1.1",
    "sequelize": "^3.30.2",
    "sequelize-cli": "^2.5.1",
    "simple-oauth2": "^1.0.3"
  }
}
